<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="zhy_button_submit_level_1">
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimen_32dp</item>
        <item name="android:background">@drawable/zhy_button_state_5</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">@dimen/dimen_688dp</item>
        <item name="android:layout_height">@dimen/dimen_80dp</item>
    </style>

    <style name="order_button_level_1">
        <item name="android:layout_marginLeft">@dimen/dimen_8dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_8dp</item>
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/zhy_button_state_5</item>
    </style>

    <style name="order_button_level_2">
        <item name="android:layout_marginLeft">@dimen/dimen_8dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:textColor">@color/zhy_text_color_1</item>
        <item name="android:background">@drawable/zhy_button_state_2</item>
    </style>

    <style name="order_button_level_3">
        <item name="android:layout_marginLeft">@dimen/dimen_8dp</item>
        <item name="android:layout_marginRight">@dimen/dimen_8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/zhy_button_bg_uncheck_gray</item>
    </style>

    <style name="orderlist_item_btn_textStyle" parent="@android:style/Widget.TextView">
        <item name="android:textColor">#7b5237</item>
        <item name="android:textSize">@dimen/dimen_24dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/zhy_button_state_6</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <style name="zhy_tablayout_textstyle" parent="@android:style/TextAppearance.Holo.Large">
        <item name="android:textColor">@color/zhy_text_color_1</item>
        <item name="android:textSize">@dimen/dimen_32dp</item>
    </style>

    <style name="orderlist_text_28dp_1">
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:textColor">@color/dimgray</item>
    </style>

    <style name="orderlist_text_28dp_2">
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:textColor">@color/zhy_text_color_1</item>
    </style>

    <style name="orderlist_text_32dp_1">
        <item name="android:textSize">@dimen/dimen_32dp</item>
        <item name="android:textColor">@color/zhy_text_color_4</item>
    </style>

    <style name="orderlist_text_32dp_2">
        <item name="android:textSize">@dimen/dimen_32dp</item>
        <item name="android:textColor">@color/zhy_text_color_1</item>
    </style>


    <style name="store_order_details_text_1">
        <item name="android:textSize">@dimen/dimen_24dp</item>
        <item name="android:textColor">@color/zhy_text_color_18</item>
        <item name="android:paddingLeft">@dimen/dimen_16dp</item>
        <item name="android:paddingRight">@dimen/dimen_16dp</item>
        <item name="android:paddingTop">@dimen/dimen_14dp</item>
        <item name="android:paddingBottom">@dimen/dimen_14dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="store_order_list_item_text">
        <item name="android:textColor">@color/black1</item>
        <item name="android:textSize">@dimen/dimen_28dp</item>
        <item name="android:layout_marginBottom">@dimen/dimen_7dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_7dp</item>

    </style>
</resources>